// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Gradio app",
        "type": "python",
        "request": "launch",
        "module": "src.app",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "justMyCode": true,
        "env": {
          "GRADIO_SERVER_NAME": "0.0.0.0"
        },
      }
    ]
  }
  